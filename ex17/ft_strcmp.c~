int	ft_strcmp(char *s1, char *s2)
{
	int	i;

	i = 0;
	while (s1[i] == s2[i] && s1[i] && s2[i])
		i++;
	return (s1[i] - s2[i]);	
}

#include <stdio.h>

int	main()
{
	printf("%d\n", ft_strcmp("aaa", "aaa"));
	printf("%d\n", ft_strcmp("abc", "aaa"));
	printf("%d\n", ft_strcmp("aaa", "cba"));
	printf("%d\n", ft_strcmp("aaaezez", "aaa"));
}
