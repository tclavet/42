#include <stdlib.h>

int	*ft_range(int min, int max)
{
	int	*tab;
	int	i;

	if (min >= max)
		return(NULL);
	tab = malloc(sizeof(*tab) * ((max - min) + 1));
	i = 0;
	while (min < max)
	{
		tab[i] = min;
		i++;
		min++;
	}
	tab[i] = 0;
	return (tab);
}

#include <stdio.h>

int	main()
{
	int 	i;
	int	*tab;

	i = 0;
	tab = ft_range(11, 10);

	while (i < 10)
	{
		printf("%d\n", tab[i]);
		i++;
	}
}
